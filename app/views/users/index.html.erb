<h1>Listing users</h1>
<%= link_to 'Login', new_user_path %>
<table>
  <tr>
    <th>Email</th>
    <th>Password</th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.password %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
  <% end %>
</table>